function refreshlist(reviewHTML) {
  const newlist = document.getElementById('desks');
  newlist.innerText =''
  newlist.insertAdjacentHTML('beforeend', reviewHTML);
}

refreshlist('<%= j render "bookings/desks", desks: @desks %>');

